{
  "name": "@building-maintenance-app/mobile",
  "version": "0.1.0",
  "private": true,
  "description": "Mobile app for Building Maintenance App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace BuildingMaintenanceApp.xcworkspace -scheme BuildingMaintenanceApp -configuration Release"
  },
  "dependencies": {
    "@building-maintenance-app/shared": "*",
    "@react-native-async-storage/async-storage": "^1.0.0",
    "@react-native-community/datetimepicker": "^7.0.0",
    "@react-native-community/image-picker": "^7.0.0",
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@tanstack/react-query": "^4.0.0",
    "axios": "^1.0.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-gesture-handler": "^2.0.0",
    "react-native-paper": "^5.0.0",
    "react-native-safe-area-context": "^4.0.0",
    "react-native-screens": "^3.0.0",
    "react-redux": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "metro-react-native-babel-preset": "0.76.0",
    "prettier": "^2.8.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}